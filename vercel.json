{
  "buildCommand": "echo 'Static files only'",
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/dashboard.html"
    }
  ],
  "redirects": [
    {
      "source": "/api/:path*",
      "destination": "https://tcs-voleyball-backend.railway.app/api/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}
